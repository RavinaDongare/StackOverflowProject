@model List<StackOverflowProject.ViewModels.CategoryViewModel>

@{
    ViewBag.Title = "Category";
}

<div class="container table">
    <h2>Categories</h2>
    <div class="panel panel-default">
        <div class="panel-body">
            <ul class=" list-group list-group-flush">
                @foreach (var item in Model)
                {

                    <li class="list-group-item list-group-item-action"> @item.CategoryID . @item.CategoryName    </li>
                }

            </ul>
        </div>
    </div>
</div>

<script>
    $("a[href = '/home/Catrgories']").attr("id", "active");
</script>